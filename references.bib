@article{https://doi.org/10.1111/ajgw.12197,
  title = {Role of Vineyard Practices in Generating and Mitigating Greenhouse Gas Emissions},
  author = {Longbottom, M.L. and Petrie, P.R.},
  year = {2015},
  journal = {Australian Journal of Grape and Wine Research},
  volume = {21},
  number = {S1},
  eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/ajgw.12197},
  pages = {522--536},
  doi = {10.1111/ajgw.12197},
  abstract = {Abstract Climate change is expected to have a significant impact on the wine industry, and as an industry, it is critical that practices are evaluated to ensure opportunities to mitigate greenhouse gas (GHG) emissions are identified. Data from Australian vineyards were examined and compared with international data to identify the sources of GHG emissions in viticulture. These were also compared with other annual and perennial agricultural crops to evaluate the relative emissions from viticulture. Fuel and electricity use are significant contributors to the total GHG emissions from viticulture (approximately 98\%), with nitrous oxide emissions being low. The greatest opportunity to reduce GHG emissions from viticulture is the reduction of carbon-based energy sources. Nitrous oxide emissions may be further reduced by modifying the timing of nitrogen fertiliser application. Soil carbon sequestration opportunities also exist, namely compost and mulch application; however, increase in soil carbon is only expected after many years of treatment.},
  keywords = {carbon,climate,greenhouse gas,nitrous oxide,soil,viticulture}
}

