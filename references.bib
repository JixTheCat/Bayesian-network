@article{palaciosDeepLearningComputer2022,
  title = {Deep Learning and Computer Vision for Assessing the Number of Actual Berries in Commercial Vineyards},
  author = {Palacios, Fernando and {Melo-Pinto}, Pedro and Diago, Maria P. and Tardaguila, Javier},
  year = {2022},
  journal = {Biosystems engineering},
  volume = {218},
  pages = {175--188},
  publisher = {Elsevier Ltd},
  address = {SAN DIEGO},
  issn = {1537-5110},
  doi = {10.1016/j.biosystemseng.2022.04.015},
  abstract = {The number of berries is one of the most relevant yield components that drives grape production in viticulture. The goal of this work was to estimate the number of actual berries per grapevine using computer vision and deep learning in commercial vineyards. Images from the visible range (RGB) were acquired from a set of 96 grapevines (Vitis vinifera L.) at pea-size berry stage using a red, green and blue camera (RGB). At harvest, the number of berries and per vine was manually assessed as the ground-truth values. The algorithm involved computer vision to detect berries in the images and to extract canopy features, in order to gain information about canopy occlusion. These were used by the machine learning regression models built to estimate the number of actual berries per vine. A SegNet architecture was used to segment individual berries and several canopy related features. Four datasets were created combining the number of estimated visible berries and different canopy features. Three different regression models were tested on the four datasets. The best results were achieved with support vector regression (SVR) on a dataset including six canopy features. This method yielded a root mean squared error (RMSE) of 205 berries, a normalised root mean squared error (NRMSE) of 24.99\% and a coefficient of determination (R2) of 0.83 between the number of estimated and the number of actual berries per vine. The results show that the number of actual berries in grapevines can be assessed with high accuracy up to 60 days prior to grape harvest, using the developed algorithm based on computer vision and deep learning. [Display omitted] {$\bullet$}The number of actual berries was estimated per vine 60 days before harvest.{$\bullet$}Machine learning models used canopy features to overcome canopy occlusions.{$\bullet$}A Deep learning algorithm quantified visible berries and segmented canopy elements.{$\bullet$}RGB vine images were acquired on-the-go on a commercial vineyard.},
  keywords = {Agricultural Engineering,Agriculture,Agriculture Multidisciplinary,Algorithms,Data mining,Grapes,Grapevine yield components,Harvesting,Life Sciences & Biomedicine,Machine learning,Machine vision,Non-invasive sensing technologies,Precision viticulture,Science & Technology,SegNet architecture,Vineyards,Wine industry,Wineries}
}

